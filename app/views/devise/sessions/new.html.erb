<div class="signup">
  <%= render '/home/header' %>
  <div class="signup-content">
    <h2 class="signup-content__title">ログイン</h2>


    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.label :email, class: "field-text" %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "field-form" %>
      </div>

      <div class="field">
        <%= f.label :パスワード, class: "field-text" %><br />
        <%= f.password_field :password, autocomplete: "current-password", class: "field-form" %>
      </div>

      <%# <% if devise_mapping.rememberable? %>
        <%# <div class="field"> %>
          <%# <%= f.check_box :remember_me %>
          <%# <%= f.label :remember_me %>
        <%# </div> %>
      <%# <% end %> 

      <div class="actions">
        <%= f.submit "ログインする", class: "submit" %>
      </div>
    <% end %>

    <%# <%= render "devise/shared/links" %>
  </div>
</div>